            <else-if type="article-newspaper">
              <text variable="URL"/>
            </else-if>
          </choose>
        </group>
      </group>
    </group>
  </macro>


  <!-- macros for zh citation -->

  <macro name="std-locator-zh">
    <choose>
      <if locator="page">
        <number variable="locator" prefix="頁"/>
      </if>
      <else-if locator="article">
        <number variable="locator" prefix="第" suffix="條"/>
      </else-if>
      <else-if locator="section">
        <choose>
          <if type="bill legislation regulation">
            <number variable="locator" prefix="第" suffix="條"/>
          </if>
          <else>
            <number variable="locator" prefix="第" suffix="節"/>
          </else>
        </choose>
      </else-if>
      <else-if locator="chapter">
        <number variable="locator" prefix="第" suffix="章"/>
      </else-if>
      <else-if locator="paragraph">
        <number variable="locator" prefix="第" suffix="段"/>
      </else-if>
      <else>
        <text variable="locator"/>
      </else>
    </choose>
  </macro>

  <macro name="jurisdiction-name-zh">
    <choose>
      <if jurisdiction="tw">
        <text value="中華民國"/>
      </if>
      <else-if jurisdiction="cn">
        <text value="中國"/>
      </else-if>
      <else-if jurisdiction="sg">
        <text value="新加坡"/>
      </else-if>
      <else-if jurisdiction="hk">
        <text value="香港"/>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-author">
    <names variable="author">
      <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
      <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
    </names>
  </macro>

  <macro name="tw-recipient">
    <names variable="recipient">
      <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
      <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
    </names>
  </macro>

  <macro name="tw-authority">
    <names variable="authority">
      <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
      <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
    </names>
  </macro>

  <macro name="tw-secondary-author">
    <choose>
      <if>
        <conditions match="none">
          <condition match="any" type="interview personal_communication broadcast motion_picture video"/>
          <condition match="all" type="song" genre="podcast"/>
        </conditions>
        <names variable="author">
          <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          <substitute>
            <names variable="container-author"/>
            <names variable="composer"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>

  <macro name="tw-secondary-pretitle">
    <choose>
      <if type="motion_picture">
        <text value="電影"/>
      </if>
      <else-if match="all" type="broadcast" genre="television-broadcast">
        <!-- text term="television-broadcast"/ -->
        <text value="電視節目"/>
      </else-if>
      <else-if match="all" type="broadcast" genre="radio-broadcast">
        <!-- text term="radio-broadcast"/ -->
        <text value="廣播節目"/>
      </else-if>
      <else-if match="all" type="song" genre="podcast">
        <!-- text term="podcast"/ -->
        <text value="網路廣播"/>
      </else-if>
      <else-if type="video">
        <text value="影片"/>
      </else-if>
      <else-if type="interview">
        <group delimiter="">
          <text macro="tw-author"/>
          <text variable="medium"/>
          <text form="verb" term="interview"/>
        </group>
      </else-if>
      <else-if type="personal_communication">
        <group delimiter="">
          <text macro="tw-author"/>
          <text macro="tw-recipient" prefix="致"/>
          <choose>
            <if variable="genre">
              <choose>
                <if genre="email">
                  <text term="email"/>
                </if>
                <else-if genre="instant-message">
                  <text term="instant-message"/>
                </else-if>
                <else>
                  <text variable="genre"/>
                </else>
              </choose>
            </if>
            <else>
              <text term="letter"/>
            </else>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-secondary-title">
    <group delimiter="，">
      <text macro="tw-secondary-pretitle"/>
      <group delimiter=" ">
        <choose>
          <if>
            <conditions match="any">
              <condition type="article-journal article-newspaper article-magazine chapter entry-encyclopedia entry-dictionary post webpage" match="any"/>
              <condition type="report broadcast song" variable="container-title" match="all"/>
              <condition type="song" genre="podcast" variable="collection-title" match="all"/>
            </conditions>
            <text variable="title" prefix="〈" suffix="〉"/>
          </if>
          <else-if type="book report broadcast song motion_picture">
            <text variable="title" prefix="《" suffix="》"/>
          </else-if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
        <choose>
          <if match="all" type="song" variable="composer">
            <names variable="composer" prefix="(" suffix="曲)"/>
          </if>
        </choose>
      </group>

      <!-- post-title -->
      <choose>
        <if type="patent">
          <text variable="title"/>
          <group delimiter="">
            <text macro="jurisdiction-name-zh"/>
            <number variable="number" prefix="專利第" suffix="號"/>
          </group>
        </if>
        <!-- for computer program (type "book") only -->
        <else-if>
          <conditions match="all">
            <condition match="all" type="book" variable="title version"/>
            <condition match="any" disambiguate="true" position="first" context="bibliography"/>
          </conditions>
          <choose>
            <if is-numeric="version">
              <text variable="version" prefix="第" suffix="版"/>
            </if>
            <else>
              <text variable="version"/>
            </else>
          </choose>
          <text form="short" variable="medium"/>
        </else-if>
      </choose>
    </group>
  </macro>

  <macro name="tw-edition">
    <choose>
      <if is-numeric="edition">
        <number form="long-ordinal" variable="edition" suffix="版"/>
      </if>
      <else>
        <number variable="edition"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-issued">
    <group delimiter="，" prefix="(" suffix=")">
      <choose>
        <if type="graphic broadcast motion_picture song video">
          <text variable="medium"/>
        </if>
      </choose>
      <choose>
        <if type="graphic">
          <text variable="dimensions"/>
        </if>
      </choose>
      <names delimiter="，" variable="interviewer editor translator">
        <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
        <label form="short"/>
      </names>
      <choose>
        <if type="song"/>
        <else-if type="thesis">
          <text variable="genre"/>
          <text variable="publisher"/>
        </else-if>
        <else-if type="report">
          <text macro="tw-authority"/>
        </else-if>
        <else-if variable="collection-title">
          <text variable="collection-title"/>
          <group>
            <choose>
              <if is-numeric="collection-number">
                <label form="long" variable="volume"/>
              </if>
            </choose>
            <number label-form="short" variable="collection-number"/>
          </group>
        </else-if>
      </choose>
      <group>
        <text term="in"/>
        <text form="short" variable="archive"/>
      </group>
      <text variable="archive_location"/>
      <group delimiter="，">
        <group delimiter="，">
          <choose>
            <if match="none" type="thesis">
              <group delimiter="，" subgroup-delimiter="；">
                <choose>
                  <if type="chapter book manuscript" match="none">
                    <text variable="publisher-place"/>
                  </if>
                </choose>
                <choose>
                  <if context="bibliography">
                    <text form="short" variable="publisher"/>
                  </if>
                </choose>
                <text macro="tw-edition"/>
              </group>
            </if>
          </choose>
        </group>
        <choose>
          <if match="none" variable="locator-revision">
            <text macro="tw-issued-date"/>
          </if>
        </choose>
      </group>
    </group>
  </macro>

  <macro name="tw-issued-date">
    <choose>
      <if match="any" has-year-only="issued" type="legal_case">
        <date date-parts="year" form="numeric" variable="issued"/>
      </if>
      <else-if type="treaty">
        <group delimiter="，">
          <date date-parts="year-month-day" form="text" variable="event-date" suffix="簽署"/>
          <date date-parts="year-month-day" form="text" variable="issued" suffix="生效"/>
        </group>
      </else-if>
      <else-if type="patent">
        <group delimiter="，">
          <date date-parts="year-month-day" form="text" variable="submitted" suffix="申請"/>
          <date date-parts="year-month-day" form="text" variable="issued" suffix="生效"/>
        </group>
      </else-if>
      <else>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </else>
    </choose>
  </macro>

  <macro name="ibid-with-locator-label-zh">
        <group delimiter="，">
          <text term="ibid"/>
          <text macro="std-locator-zh"/>
        </group>
  </macro>

  <macro name="hereinafter-zh">
    <choose>
      <if variable="hereinafter title-short">
        <group prefix="(" suffix=")">
          <text value="以下簡稱"/>
          <choose>
            <if variable="hereinafter">
              <text variable="hereinafter" prefix="「" suffix="」"/>
            </if>
            <else>
              <text variable="title-short" prefix="「" suffix="」"/>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="hereinafter-stub-zh">
    <choose>
      <if variable="hereinafter">
        <text variable="hereinafter"/>
      </if>
      <else-if variable="title-short">
        <text variable="title-short"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-primary-subsequent">
    <group delimiter="，">
      <text macro="hereinafter-stub-zh"/>
      <choose>
        <if match="none" type="legislation regulation treaty">
          <text macro="tw-supra-stub"/>
        </if>
      </choose>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="tw-primary-first">
    <text macro="tw-primary-bib"/>
    <text macro="hereinafter-zh"/>
  </macro>

  <macro name="tw-primary-bib">
    <group delimiter=" ">
      <group delimiter="，">
        <group delimiter="">
          <choose>
            <if match="none" jurisdiction="tw">
              <text macro="jurisdiction-name-zh"/>
            </if>
          </choose>
          <choose>
            <if match="all" type="bill" variable="author">
                <text macro="tw-author" suffix="版"/>
            </if>
          </choose>
          <choose>
            <if>
              <conditions match="any">
                <condition match="none" jurisdiction="tw"/>
                <condition match="all" type="bill" variable="author"/>
              </conditions>
              <text variable="title" prefix="「" suffix="」"/>
            </if>
            <else>
              <text variable="title"/>
            </else>
          </choose>
        </group>
        <choose>
          <if type="hearing">
            <choose>
              <if variable="author">
                <text macro="tw-author" suffix="發言紀錄"/>
              </if>
              <else>
                <text value="會議紀錄"/>
              </else>
            </choose>
          </if>
        </choose>
        <text macro="std-locator-zh"/>
      </group>
      <choose>
        <if match="none" type="legislation">
          <group delimiter="，" prefix="(" suffix=")">
            <choose>
              <if type="hearing">
                <text macro="tw-authority" suffix="主辦"/>
              </if>
            </choose>
            <text macro="tw-issued-date"/>
          </group>
        </if>
      </choose>
    </group>
  </macro>

  <macro name="tw-supra-stub">
    <group delimiter="，">
      <text variable="first-reference-note-number" prefix="前揭註"/>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="tw-presentation-description">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event"/>
      <else-if variable="event">
        <choose>
          <if variable="title">
            <text term="presented at"/>
          </if>
          <else-if variable="genre">
            <text form="short" variable="genre"/>
          </else-if>
          <else>
            <text term="presented at"/>
          </else>
        </choose>
        <text text-case="title" variable="event"/>
      </else-if>
    </choose>
  </macro>

  <macro name="jurisdiction-filter-zh">
    <choose>
      <if match="any" position="first">
        <text macro="tw-primary-first"/>
      </if>
      <else>
        <text macro="tw-primary-subsequent"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-container">
    <choose>
      <if>
        <conditions match="any">
          <condition variable="container-title"/>
          <condition type="song" variable="collection-title" match="all"/>
        </conditions>
        <group delimiter="，">
          <group delimiter=" ">
            <group delimiter="">
              <choose>
                <if type="chapter song">
                  <text term="in"/>
                </if>
              </choose>
              <choose>
                <if>
                  <conditions match="all">
                    <condition type="chapter" variable="container-author" match="all"/>
                    <condition variable="author" match="none"/>
                  </conditions>
                  <text value="氏著"/>
                </if>
              </choose>
              <choose>
                <if variable="container-title">
                  <text variable="container-title" prefix="《" suffix="》"/>
                </if>
                <else-if match="all" type="song" variable="collection-title">
                  <text variable="collection-title" prefix="《" suffix="》"/>
                </else-if>
              </choose>
              <choose>
                <if>
                  <conditions match="any">
                    <condition type="broadcast"/>
                    <condition type="song" genre="podcast" match="all"/>
                  </conditions>
                  <group>
                    <number variable="number" prefix="第" suffix="集"/>
                  </group>
                </if>
                <else-if match="all" variable="volume issue">
                  <number variable="volume" suffix="卷"/>
                  <number variable="issue" suffix="期"/>
                </else-if>
                <else>
                  <number variable="volume" prefix="第" suffix="卷"/>
                  <number variable="issue" prefix="第" suffix="期"/>
                </else>
              </choose>
            </group>
            <choose>
              <if>
                <conditions match="all">
                  <condition type="article-newspaper"/>
                  <condition match="any" variable="edition section"/>
                </conditions>
                <group delimiter="，" prefix="(" suffix=")">
                  <text macro="tw-edition"/>
                  <choose>
                    <if is-numeric="section">
                      <text variable="section" prefix="第" suffix="落"/>
                    </if>
                    <else>
                      <text variable="section"/>
                    </else>
                  </choose>
                </group>
              </if>
            </choose>
          </group>
          <choose>
            <if type="article-newspaper">
              <date date-parts="year-month-day" form="text" variable="issued"/>
              <number variable="page-first" suffix="版"/>
            </if>
            <else>
              <choose>
                <if context="citation">
                  <number variable="page-first" prefix="頁"/>
                </if>
                <else>
                  <!-- <number variable="page-first" prefix="頁"/> doesn't work for there would be two "頁" -->
                  <group prefix="頁">
                    <number variable="page"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="tw-url">
    <choose>
      <if type="graphic post-weblog webpage post entry-dictionary entry-encyclopedia broadcast motion_picture video song map">
        <group delimiter=" ">
          <choose>
            <if type="post-weblog webpage post entry-dictionary entry-encyclopedia"/>
            <else-if match="all" type="song" variable="collection-title">
              <choose>
                <if variable="container-title">
                  <text value="亦見於"/>
                </if>
              </choose>
            </else-if>
            <else-if variable="publisher">
              <text value="亦見於"/>
            </else-if>
          </choose>
          <text variable="URL"/>
        </group>
      </if>
      <else-if match="all" type="book" variable="version">
        <text variable="URL"/>
      </else-if>
      <else-if match="all" type="article-newspaper" variable="URL">
        <choose>
          <if variable="page-first">
            <group delimiter=" ">
              <text value="亦見於"/>
              <text variable="URL"/>
            </group>
          </if>
          <else>
            <text variable="URL"/>
          </else>
        </choose>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-secondary-subsequent">
    <group delimiter="，">
      <text macro="tw-secondary-author"/>
      <choose>
        <if variable="hereinafter">
          <text variable="hereinafter"/>
        </if>
        <else-if>
          <conditions match="any">
            <condition match="none" variable="author composer container-author"/>
            <condition type="motion_picture broadcast"/>
            <condition match="all" type="song" genre="podcast"/>
          </conditions>
          <text form="short" variable="title"/>
        </else-if>
      </choose>
      <text macro="tw-supra-stub"/>
    </group>
  </macro>

  <macro name="tw-secondary-first">
      <text macro="tw-secondary-author" suffix="，"/>
      <text macro="tw-secondary-main"/>
  </macro>

  <macro name="tw-secondary-main">
    <group delimiter="，">
      <group delimiter=" ">
        <group delimiter="，">
          <text macro="tw-secondary-title"/>
          <text macro="opus-number"/>
          <text macro="tw-presentation-description"/>
          <text macro="tw-container"/>
          <text macro="std-locator-zh"/>
        </group>
        <choose>
          <if match="none" type="classic article-newspaper">
            <text macro="tw-issued"/>
          </if>
        </choose>
        <text macro="hereinafter-zh"/>
      </group>
      <text macro="tw-url"/>
    </group>
  </macro>

  <!-- macros for zh bibliography -->

  <macro name="tw-secondary-bib">
    <text macro="tw-secondary-author" suffix="。"/>
    <text macro="tw-secondary-main"/>
  </macro>

  <!-- macros for bibliography sorting -->

  <macro name="bib-sort-key-author-title">
    <choose>
      <if locale="zh">
        <!-- a space leading off Chinese items so they'll be grouped at the front -->
        <text value=" "/>
        <choose>
          <if type="bill hearing legislation legal_case regulation treaty">
            <choose>
              <if match="none" jurisdiction="tw">
                <text macro="jurisdiction-name-zh"/>
              </if>
            </choose>
            <choose>
              <if type="bill">
                <text macro="tw-author" suffix="版"/>
              </if>
            </choose>
          </if>
          <else-if match="none" variable="author container-author composer">
            <text macro="tw-secondary-pretitle"/>
          </else-if>
          <else>
            <text macro="tw-secondary-author"/>
            <text macro="tw-secondary-pretitle"/>
          </else>
        </choose>
        <text variable="title"/>
      </if>
      <else>
        <choose>
          <if type="interview">
            <text macro="us-interview-author"/>
          </if>
          <else>
            <text macro="us-default-author-bib"/>
          </else>
        </choose>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <citation disambiguate-add-givenname="true" et-al-min="3" et-al-use-first="1" near-note-distance="5">

    <layout locale="zh-TW zh" delimiter="；" suffix="。">
      <choose>
        <if position="ibid-with-locator">
          <text macro="ibid-with-locator-label-zh"/>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if match="any" type="bill hearing legislation legal_case regulation treaty">
          <text macro="jurisdiction-filter-zh"/>
        </else-if>
        <else-if match="none" position="first" type="classic">
          <text macro="tw-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="tw-secondary-first"/>
        </else>
      </choose>
    </layout>

    <!-- en citation layout from amlaw.csl -->
    <layout delimiter="; " suffix=".">
      <choose>
        <if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter"/>
        </if>
        <else-if position="ibid-with-locator">
          <text macro="ibid-with-locator-label"/>
        </else-if>
        <else-if position="ibid">
          <text font-style="italic" term="ibid"/>
        </else-if>
        <else-if match="none" position="first" context="bibliography" type="classic">
          <text macro="wisconsin-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="wisconsin-secondary-first"/>
        </else>
      </choose>
    </layout>

  </citation>


  <bibliography hanging-indent="true" et-al-min="5" et-al-use-first="4" subsequent-author-substitute="———">
    <sort>
      <key macro="bib-sort-key-author-title"/>
      <key variable="issued"/>
    </sort>

    <layout locale="zh-TW zh" suffix="。">
      <choose>
        <if match="any" type="bill hearing legislation legal_case regulation treaty">
          <text macro="tw-primary-bib"/>
        </if>
        <else>
          <text macro="tw-secondary-bib"/>
        </else>
      </choose>
    </layout>
    <layout suffix=".">
      <choose>
        <if match="any" type="treaty legal_case legislation gazette regulation" variable="jurisdiction">
          <text macro="jurisdiction-filter"/>
        </if>
        <else>
          <text macro="wisconsin-secondary-bib"/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>

