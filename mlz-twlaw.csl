  <!-- macros for zh citation -->

  <macro name="std-locator-zh">
    <choose>
      <if locator="page">
        <number variable="locator" prefix="頁"/>
      </if>
      <else-if locator="paragraph">
        <number variable="locator" prefix="第" suffix="段"/>
      </else-if>
      <else-if locator="chapter">
        <number variable="locator" prefix="第" suffix="章"/>
      </else-if>
      <else-if locator="article">
        <number variable="locator" prefix="第" suffix="條"/>
      </else-if>
      <else-if locator="section">
        <choose>
          <if type="bill legislation regulation">
            <number variable="locator" prefix="第" suffix="條"/>
          </if>
          <else>
            <number variable="locator" prefix="第" suffix="節"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text variable="locator"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-container-author">
    <names variable="container-author">
      <name and="symbol" delimiter-precedes-last="never" initialize="false" initialize-with="."/>
      <institution institution-parts="short" use-first="1" use-last="1"/>
    </names>
  </macro>

  <macro name="tw-author">
    <choose>
      <if type="patent">
        <group delimiter="；">
          <names variable="author" suffix=" (發明人)">
            <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          </names>
          <choose>
            <if context="bibliography">
              <names variable="recipient" suffix=" (專利權人)"/>
            </if>
          </choose>
        </group>
      </if>
      <else-if match="all" type="bill" variable="author">
        <names variable="author" suffix="版"/>
        <text variable="title" prefix="「" suffix="」"/>
      </else-if>
      <else-if type="bill hearing legislation legal_case regulation treaty">
          <text variable="title"/>
      </else-if>
      <else-if type="broadcast">
          <text variable="genre"/>
          <group delimiter="：">
            <group>
              <text variable="container-title" prefix="《" suffix="》"/>
              <number variable="number" prefix="第" suffix="集"/>
            </group>
            <text variable="title"/>
          </group>
      </else-if>
      <else-if type="motion_picture">
          <group delimiter="：">
            <text value="電影"/>
            <text variable="title" prefix="《" suffix="》"/>
          </group>
      </else-if>
      <else-if type="video">
          <group delimiter="：">
            <text variable="medium"/>
            <text variable="title"/>
          </group>
      </else-if>
      <else-if type="interview">
        <group delimiter="">
          <names variable="author">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <text variable="medium"/>
          <text form="verb" term="interview"/>
        </group>
      </else-if>
      <else-if type="letter">
        <group delimiter="">
          <names variable="author">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <names variable="recipient" prefix="致">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <text value="書信"/>
        </group>
      </else-if>
      <else-if type="personal_communication">
        <group delimiter="">
          <names variable="author">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <names variable="recipient" prefix="致">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution delimiter="，" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <text variable="genre"/>
        </group>
      </else-if>
      <else-if variable="translator">
        <group delimiter="，">
          <names variable="author" suffix="著">
            <name and="symbol" delimiter=", " delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
            <substitute>
              <names variable="container-author"/>
            </substitute>
          </names>
          <names variable="translator" suffix="譯">
            <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          </names>
        </group>
      </else-if>
      <else>
        <names variable="author">
          <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          <substitute>
            <names variable="container-author"/>
            <names variable="composer"/>
            <text variable="title"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>

  <macro name="tw-title-version">
    <group delimiter=", ">
      <choose>
        <if is-numeric="version">
          <text variable="version" prefix="第" suffix="版" />
          <text form="short" variable="medium"/>
        </if>
        <else>
          <text variable="version"/>
          <text form="short" variable="medium"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tw-default-title">
    <choose>
      <!-- group 1, items having "used up" their "title" in "tw-author" -->
      <if>
        <conditions match="any">
          <condition type="bill broadcast video motion_picture legislation legal_case regulation treaty" match="any"/>
          <condition variable="author container-author composer translator" match="none"/>
        </conditions>
      </if>
      <else-if type="hearing">
        <choose>
          <if variable="author">
            <names variable="author" suffix="發言紀錄">
              <name and="symbol" delimiter="、" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
            </names>
          </if>
          <else>
            <text value="會議紀錄"/>
          </else>
        </choose>
      </else-if>

      <!-- group 2, others -->
      <else>
        <choose>
          <if type="patent">
            <group delimiter="，">
              <text variable="title"/>
              <number variable="number" prefix="中華民國專利第" suffix="號"/>
            </group>
              </if>
          <else-if type="book song report">
            <choose>
              <!-- version check for computer program (type book) only -->
              <if match="all" variable="title version">
                <group delimiter=", ">
                  <text variable="title" prefix="《" suffix="》"/>
                  <choose>
                    <if disambiguate="true">
                      <text macro="tw-title-version"/>
                    </if>
                    <else-if position="subsequent"/>
                    <else>
                      <text macro="tw-title-version"/>
                    </else>
                  </choose>
                </group>
              </if>
              <else-if variable="container-title">
                  <text variable="title" prefix="〈" suffix="〉"/>
              </else-if>
              <else>
                <text variable="title" prefix="《" suffix="》"/>
              </else>
              <names variable="composer" prefix="【" suffix="】"/>
            </choose>
          </else-if>
          <else-if type="article-journal article-newspaper article-magazine chapter entry-encyclopedia entry-dictionary post webpage" match="any">
            <text variable="title" prefix="〈" suffix="〉"/>
          </else-if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>

  <macro name="tw-title-short">
    <choose>
      <if>
        <conditions match="any">
          <condition match="none" variable="author composer editor container-author"/>
          <condition type="motion_picture broadcast"/>
          <!-- condition disambiguate="true"/ -->
        </conditions>
        <choose>
          <if variable="hereinafter">
            <text variable="hereinafter"/>
          </if>
          <else-if variable="title-short">
            <text form="short" variable="title"/>
          </else-if>
          <else>
            <text macro="tw-default-title"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>

  <macro name="tw-editor">
    <choose>
      <if type="broadcast">
        <group>
          <names delimiter="，" variable="author">
            <name delimiter-precedes-last="never"/>
          </names>
          <text term="director"/>
        </group>
      </if>
      <else>
        <names delimiter="，" variable="director editor">
          <name delimiter-precedes-last="never"/>
          <label form="short"/>
        </names>
      </else>
    </choose>
  </macro>

  <macro name="zh-edition">
    <choose>
      <if is-numeric="edition">
        <number form="long-ordinal" variable="edition" suffix="版"/>
      </if>
      <else>
        <number variable="edition"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-issued">
    <choose>
      <if match="none" type="classic">
        <text macro="tw-issued-content" prefix="(" suffix=")"/>
      </if>
    </choose>
  </macro>

  <macro name="tw-issued-content">
    <group delimiter="，">
      <choose>
        <if type="graphic song broadcast video">
          <text variable="medium"/>
        </if>
      </choose>
      <choose>
        <if type="graphic">
          <text variable="dimensions"/>
        </if>
        <else-if type="song broadcast video">
          <text variable="genre"/>
        </else-if>
      </choose>
      <names variable="interviewer" prefix="訪談者："/>
      <text macro="tw-editor"/>
      <choose>
        <if type="song"/>
        <else-if type="thesis">
            <text variable="genre"/>
            <text variable="publisher"/>
        </else-if>
        <else-if type="report">
            <names variable="authority" suffix="委託研究"/>
        </else-if>
        <else-if variable="collection-title">
          <text variable="collection-title"/>
          <group>
            <choose>
              <if is-numeric="collection-number">
                <label form="long" variable="volume"/>
              </if>
            </choose>
            <number label-form="short" variable="collection-number"/>
          </group>
        </else-if>
      </choose>
      <group>
        <text term="in"/>
        <text form="short" variable="archive"/>
      </group>
      <text variable="archive_location"/>
      <group delimiter="，">
        <group delimiter="，">
          <choose>
            <if match="none" type="thesis">
              <group delimiter="，" subgroup-delimiter="；">
                <choose>
                  <if type="chapter book manuscript" match="none">
                    <text variable="publisher-place"/>
                  </if>
                </choose>
                <choose>
                  <if context="bibliography">
                    <text form="short" variable="publisher"/>
                  </if>
                </choose>
                <text macro="zh-edition"/>
              </group>
            </if>
          </choose>
        </group>
        <choose>
          <if match="none" variable="locator-revision">
            <text macro="tw-issued-date"/>
          </if>
        </choose>
      </group>
    </group>
  </macro>

  <macro name="tw-issued-date">
    <choose>
      <if match="any" has-year-only="issued" type="legal_case">
        <date date-parts="year" form="numeric" variable="issued"/>
      </if>
      <else-if type="treaty">
        <group delimiter="，">
          <date date-parts="year-month-day" form="text" variable="event-date" suffix="簽署"/>
          <date date-parts="year-month-day" form="text" variable="issued" suffix="生效"/>
        </group>
      </else-if>
      <else-if type="patent">
        <group delimiter="，">
          <date date-parts="year-month-day" form="text" variable="submitted" suffix="申請"/>
          <date date-parts="year-month-day" form="text" variable="issued" suffix="生效"/>
        </group>
      </else-if>
      <else>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </else>
    </choose>
  </macro>

  <macro name="ibid-with-locator-label-zh">
        <group delimiter="，">
          <text term="ibid"/>
          <text macro="std-locator-zh"/>
        </group>
  </macro>

  <macro name="hereinafter-zh">
    <choose>
      <if variable="hereinafter title-short">
        <group prefix="(" suffix=")">
          <text value="以下簡稱"/>
          <choose>
            <if variable="hereinafter">
              <text variable="hereinafter" prefix="「" suffix="」"/>
            </if>
            <else>
              <text variable="title-short" prefix="「" suffix="」"/>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="hereinafter-stub-zh">
    <choose>
      <if variable="hereinafter">
        <text variable="hereinafter"/>
      </if>
      <else-if variable="title-short">
        <text variable="title-short"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-primary-subsequent">
      <group delimiter="，">
          <text macro="hereinafter-stub-zh"/>
          <text macro="tw-supra-stub"/>
      </group>
  </macro>

  <!-- patent assignee not mapped to recipient -->
  <macro name="tw-primary-first">
    <group delimiter=" ">
      <group delimiter="，">
        <text macro="tw-author"/>
        <text macro="tw-default-title"/>
        <text macro="std-locator-zh"/>
      </group>
      <choose>
        <if match="none" type="legislation">
          <group delimiter="，" prefix="(" suffix=")">
            <choose>
              <if type="hearing">
                <group suffix="主辦">
                  <names variable="authority"/>
                </group>
              </if>
            </choose>
            <text macro="tw-issued-date"/>
          </group>
        </if>
      </choose>
      <choose>
        <if context="citation">
          <text macro="hereinafter-zh"/>
        </if>
      </choose>
    </group>
  </macro>

  <macro name="tw-supra-stub">
    <group delimiter="，">
      <text variable="first-reference-note-number" prefix="前揭註"/>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="tw-container-title">
    <group prefix="《" suffix="》">
      <choose>
        <if match="all" type="song" variable="title container-title">
          <text term="in"/>
        </if>
      </choose>
      <choose>
        <if type="broadcast"/>
        <else-if match="all" type="video" variable="publisher"/>
        <else>
          <text form="short" variable="container-title"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tw-default-presentation-description">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event"/>
      <else-if variable="event">
        <choose>
          <if variable="title">
            <text term="presented at"/>
          </if>
          <else-if variable="genre">
            <text form="short" variable="genre"/>
          </else-if>
          <else>
            <text term="presented at"/>
          </else>
        </choose>
        <text text-case="title" variable="event"/>
      </else-if>
    </choose>
  </macro>

  <macro name="jurisdiction-filter-zh">
    <choose>
      <if type="legal_case bill legislation hearing treaty patent">
        <choose>
          <if position="subsequent">
            <text macro="tw-primary-subsequent"/>
          </if>
          <else>
            <text macro="tw-primary-first"/>
          </else>
        </choose>
      </if>
      <else-if position="subsequent">
        <text macro="tw-secondary-subsequent"/>
      </else-if>
      <else>
        <text macro="tw-secondary-first"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-collection-title-podcast">
    <choose>
      <if type="song">
        <choose>
          <if match="none" variable="container-title">
            <text variable="collection-title"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>

  <macro name="tw-secondary-subsequent">
    <group delimiter="，">
      <text macro="tw-author"/>
      <text macro="tw-title-short"/>
      <text macro="tw-supra-stub"/>
    </group>
  </macro>

  <macro name="tw-secondary-first">

    <choose>
      <if context="citation">
        <text macro="tw-author" suffix="，"/>
      </if>
      <else>
        <text macro="tw-author" suffix="。"/>
      </else>
    </choose>
    <group delimiter="，">
      <group delimiter=" ">
        <group delimiter="，">
          <text macro="tw-default-title"/>
          <text macro="tw-collection-title-podcast"/>
          <text macro="opus-number"/>
          <text macro="tw-default-presentation-description"/>
          <choose>
            <if type="webpage post manuscript">
              <text form="short" variable="genre"/>
            </if>
          </choose>
          <group>
            <choose>
              <if type="chapter">
                <text term="in"/>
              </if>
            </choose>
            <choose>
              <if>
                <conditions match="all">
                  <condition type="chapter" variable="container-author" match="all"/>
                  <condition variable="author" match="none"/>
                </conditions>
                <text macro="tw-container-title" prefix="氏著"/>
              </if>
              <else>
                <group delimiter="，">
                  <text macro="tw-container-author"/>
                  <group delimiter=" ">
                    <text macro="tw-container-title"/>
                    <choose>
                      <if type="article-newspaper">
                        <choose>
                          <if match="any" variable="edition section">
                            <group delimiter="，" prefix="(" suffix=")">
                              <text macro="zh-edition"/>
                              <choose>
                                <if is-numeric="section">
                                  <text variable="section" prefix="第" suffix="落"/>
                                </if>
                                <else>
                                  <text variable="section"/>
                                </else>
                              </choose>
                            </group>
                          </if>
                        </choose>
                      </if>
                    </choose>
                  </group>
                </group>
              </else>
            </choose>
            <group>
              <!-- don't substitute "text" with "number" below to avoid a bug in Word plugin. -->
              <!-- XXX Interesting. Let's discuss. -->
              <number variable="volume" suffix="卷"/>
              <number variable="issue" suffix="期"/>
            </group>
          </group>
          <choose>
            <if type="entry-encyclopedia entry-dictionary">
              <choose>
                <if match="none" variable="author">
                  <text variable="title"/>
                </if>
              </choose>
            </if>
          </choose>
          <choose>
            <if type="article-newspaper">
              <date date-parts="year-month-day" form="text" variable="issued"/>
              <group delimiter=", ">
                <number variable="page-first" suffix="版"/>
                <text macro="std-locator-zh"/>
              </group>
            </if>
            <else-if variable="locator">
              <text macro="std-locator-zh"/>
            </else-if>
            <else-if match="all" type="song" variable="section">
              <text macro="std-locator-zh"/>
            </else-if>
            <else-if is-numeric="title" match="all" type="song" variable="title locator">
              <text macro="std-locator-zh"/>
            </else-if>
            <else-if variable="volume">
              <choose>
                <if match="none" variable="container-title">
                  <text macro="std-locator-zh"/>
                </if>
              </choose>
            </else-if>
          </choose>
          <choose>
            <if match="none" type="article-newspaper">
              <choose>
                <if context="citation">
                  <number variable="page-first" prefix="頁"/>
                  <text macro="std-locator-zh"/>
                </if>
                <else>
                  <!-- <number variable="page-first" prefix="頁"/> doesn't work for there would be two "頁" -->
                  <group prefix="頁">
                    <number variable="page"/>
                  </group>
                </else>
              </choose>
            </if>
          </choose>
        </group>
        <choose>
          <if match="none" type="article-newspaper">
            <text macro="tw-issued"/>
          </if>
        </choose>
      </group>
      <choose>
        <if type="graphic post-weblog webpage post entry-dictionary entry-encyclopedia broadcast motion_picture video song map">
          <group delimiter=" ">
            <choose>
              <if type="post-weblog webpage post entry-dictionary entry-encyclopedia"/>
              <else-if match="all" type="song" variable="collection-title">
                <choose>
                  <if variable="container-title">
                    <text value="亦見於"/>
                  </if>
                </choose>
              </else-if>
              <else-if variable="publisher">
                <text value="亦見於"/>
              </else-if>
            </choose>
            <text variable="URL"/>
          </group>
        </if>
        <else-if match="all" type="book" variable="version">
          <text variable="URL"/>
        </else-if>
        <else-if match="all" type="article-newspaper" variable="URL">
          <choose>
            <if variable="page-first">
              <group delimiter=" ">
                <text value="亦見於"/>
                <text variable="URL"/>
              </group>
            </if>
            <else>
              <text variable="URL"/>
            </else>
          </choose>
        </else-if>
      </choose>
    </group>

  </macro>

  <macro name="bib-sort-key-author">
    <choose>
      <if locale="zh">
        <!-- a space leading off Chinese items so they'll be grouped at the front -->
        <text value=" "/>
        <choose>
          <if match="all" variable="author translator">
            <!-- another space for translated works (which start with English names) to make them sorted to the top. -->
            <!-- this is to work around a "stroke-sorting" limitation that, at least on a Mac, always sorts English words below Chinese ones. -->
            <text value=" "/>
          </if>
        </choose>
        <text macro="tw-author"/>
      </if>
      <else>
        <choose>
          <if type="interview">
            <text macro="us-interview-author"/>
          </if>
          <else>
            <text macro="us-default-author-bib"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>

  <macro name="bib-sort-key-title">
    <choose>
      <if locale="zh">
        <choose>
          <if>
            <!-- items matching either condition have "used up" their title -->
            <conditions match="none">
              <condition type="broadcast video motion_picture hearing legislation legal_case regulation treaty" match="any"/>
              <condition variable="author container-author composer translator" match="none"/>
            </conditions>
            <text variable="title"/>
          </if>
        </choose>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <citation disambiguate-add-givenname="true" et-al-min="3" et-al-use-first="1" near-note-distance="5">

    <layout locale="zh" delimiter="；" suffix="。">
      <choose>
        <if position="ibid-with-locator">
          <text macro="ibid-with-locator-label-zh"/>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter-zh"/>
        </else-if>
        <else-if match="none" position="first" type="classic">
          <text macro="tw-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="tw-secondary-first"/>
        </else>
      </choose>
    </layout>

    <!-- en citation layout from amlaw.csl -->
    <layout delimiter="; " suffix=".">
      <choose>
        <if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter"/>
        </if>
        <else-if position="ibid-with-locator">
          <text macro="ibid-with-locator-label"/>
        </else-if>
        <else-if position="ibid">
          <text font-style="italic" term="ibid"/>
        </else-if>
        <else-if match="none" position="first" context="bibliography" type="classic">
          <text macro="wisconsin-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="wisconsin-secondary-first"/>
        </else>
      </choose>
    </layout>

  </citation>
  <bibliography hanging-indent="true" et-al-min="5" et-al-use-first="4" subsequent-author-substitute="———">
    <sort>
      <key macro="bib-sort-key-author"/>
      <key macro="bib-sort-key-title"/>
      <key variable="issued"/>
    </sort>

    <layout locale="zh" suffix="。">
      <choose>
        <if type="legislation"/>
        <else-if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter-zh"/>
        </else-if>
        <else>
          <text macro="tw-secondary-first"/>
        </else>
      </choose>
    </layout>
    <layout suffix=".">
      <choose>
        <if match="any" type="treaty legal_case legislation gazette regulation" variable="jurisdiction">
          <text macro="jurisdiction-filter"/>
        </if>
        <else>
          <text macro="wisconsin-secondary-bib"/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
