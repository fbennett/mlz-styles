          <if match="none" variable="container-title">
            <text font-variant="small-caps" variable="collection-title"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>

  <!-- mod: title case for video medium -->
  <macro name="wisconsin-secondary-first">
