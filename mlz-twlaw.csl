              </choose>
              <text variable="URL"/>
            </group>
          </if>
          <else-if match="all" type="book" variable="version">
            <text variable="URL"/>
          </else-if>
          <else-if type="article-newspaper">
            <text variable="URL"/>
          </else-if>
        </choose>
      </group>
    </group>
  </macro>


