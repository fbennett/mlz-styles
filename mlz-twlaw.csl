                  <number variable="number"/>
                </group>
              </else-if>
            </choose>
            <group delimiter=" ">
              <text macro="us-default-chapter-to-book"/>
              <choose>
                <if match="none" type="book classic">
                  <number variable="volume"/>
                </if>
              </choose>
              <group delimiter=", ">
                <text font-variant="small-caps" macro="us-container-author"/>
                <text macro="us-container-title"/>
