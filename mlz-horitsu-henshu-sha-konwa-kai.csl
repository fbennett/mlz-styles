          <text macro="description-multimedia-note"/>
          <text macro="locators-newspaper"/>
          <text macro="point-locators-join-with-comma"/>
        </group>
        <text macro="point-locators-join-with-colon"/>
      </group>
      <text macro="access-note"/>
    </group>
  </macro>
  <macro name="title-note">
    <choose>
      <if match="none" type="personal_communication" variable="title">
        <text variable="genre"/>
      </if>
      <else-if match="any" type="bill legal_case" variable="jurisdiction">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report">
        <text font-style="italic" text-case="title" variable="title"/>
      </else-if>
      <else-if match="any" type="broadcast song">
        <choose>
          <if match="any" variable="container-title collection-title">
            <text quotes="true" variable="title"/>
          </if>
          <else>
            <text font-style="italic" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text quotes="true" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="secondary-contributors-note">
    <choose>
      <if match="none" type="broadcast chapter paper-conference">
        <text macro="editor-translator"/>
      </if>
      <else-if type="broadcast">
        <text macro="director-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="recipient-note">
    <names delimiter=", " variable="recipient">
      <label form="verb" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="recipient">
    <group delimiter=", ">
      <text macro="recipient-note"/>
      <choose>
        <if type="personal_communication">
          <choose>
            <if variable="genre">
              <text text-case="capitalize-first" variable="genre"/>
            </if>
            <else>
              <text term="letter" text-case="capitalize-first"/>
            </else>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="thesis">
        <text form="short" variable="publisher"/>
      </if>
      <else>
        <group delimiter=": ">
          <text variable="publisher-place" form="short"/>
          <text form="short" variable="publisher"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="point-locators-join-with-comma">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event"/>
      <else-if match="none" type="article-journal">
        <text macro="point-locators"/>
      </else-if>
    </choose>
  </macro>
  <macro name="point-locators-join-with-colon">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event">
        <text macro="point-locators"/>
      </if>
      <else-if type="article-journal">
        <choose>
          <if match="any" variable="locator page">
            <text macro="point-locators"/>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="point-locators">
    <group delimiter=" ">
      <choose>
        <if match="none" variable="locator">
          <number variable="page"/>
        </if>
        <else-if type="legal_case"/>
        <else>
          <choose>
            <if locator="page" match="none">
              <label form="short" variable="locator"/>
            </if>
          </choose>
          <number variable="locator"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="locators-note-join-with-space">
    <choose>
      <if type="article-journal">
        <text macro="locators-note"/>
      </if>
      <else-if match="all" type="paper-conference" variable="container-title event">
        <text macro="locators-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-note-join-with-comma">
    <choose>
      <if match="none" type="article-journal paper-conference">
        <text macro="locators-note"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-note-article-action">
    <group delimiter=", ">
      <number variable="volume"/>
      <group delimiter=" ">
        <text form="short" term="issue"/>
        <number variable="issue"/>
      </group>
    </group>
  </macro>
  <macro name="locators-note">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event">
        <text macro="locators-note-article-action"/>
      </if>
      <else-if type="article-journal">
        <text macro="locators-note-article-action"/>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=", ">
          <group delimiter=" ">
            <number variable="volume"/>
            <text variable="container-title"/>
            <number variable="page"/>
          </group>
          <number variable="locator"/>
        </group>
      </else-if>
      <else-if match="any" type="bill book graphic legal_case motion_picture report song chapter">
        <group delimiter=", ">
          <group delimiter=" ">
            <choose>
              <if is-numeric="volume">
                <text form="short" term="volume"/>
              </if>
            </choose>
            <number form="numeric" variable="volume"/>
          </group>
          <choose>
            <if match="none" variable="locator">
              <group delimiter=" ">
                <number form="numeric" variable="number-of-volumes"/>
                <text form="short" plural="true" term="volume"/>
              </group>
            </if>
          </choose>
          <text macro="edition-note"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-newspaper">
    <choose>
      <if type="article-newspaper">
        <group delimiter=", ">
          <text macro="edition-action"/>
          <group delimiter=" ">
            <choose>
              <if is-numeric="section">
                <text form="short" term="section"/>
              </if>
            </choose>
            <text variable="section"/>
            <choose>
              <if is-numeric="section" match="none">
                <text form="long" term="section"/>
              </if>
            </choose>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="issue-note-join-with-space">
    <choose>
      <if type="paper-conference">
        <choose>
          <if match="all" variable="container-title event">
            <text macro="issue-note"/>
          </if>
        </choose>
      </if>
      <else-if match="any" type="article-journal legal_case" variable="publisher-place publisher">
        <text macro="issue-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="issue-note-join-with-comma">
    <choose>
      <if type="paper-conference">
        <choose>
          <if match="all" variable="container-title event"/>
          <else>
            <text macro="issue-note"/>
          </else>
        </choose>
      </if>
      <else-if match="none" type="article-journal legal_case" variable="publisher-place publisher">
        <text macro="issue-note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="issue-note">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event">
        <text macro="issued" prefix="(" suffix=")"/>
      </if>
      <else-if match="any" type="article-journal legal_case">
        <text macro="issued" prefix="(" suffix=")"/>
      </else-if>
      <else-if type="manuscript"/>
      <else-if match="any" variable="publisher-place publisher">
        <group delimiter=", " prefix="(" suffix=")">
          <group delimiter=" ">
            <choose>
              <if match="none" variable="title"/>
              <else-if match="any" type="thesis speech">
                <text variable="genre"/>
              </else-if>
            </choose>
            <text macro="event"/>
          </group>
          <text macro="publisher"/>
          <text variable="locator-revision"/>
          <text macro="issued"/>
        </group>
      </else-if>
      <else>
        <text macro="issued"/>
      </else>
    </choose>
  </macro>
  <macro name="interviewer-note">
    <names delimiter=", " variable="interviewer">
      <label form="verb" suffix=" " text-case="lowercase"/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="event">
    <group delimiter=" ">
      <text term="presented at"/>
      <text variable="event"/>
    </group>
  </macro>
  <macro name="editor-translator">
    <group delimiter=", ">
      <choose>
        <if variable="author">
          <group delimiter=" ">
            <choose>
              <if variable="container-author">
                <group>
                  <names variable="container-author">
                    <label form="verb-short" suffix=" " text-case="lowercase"/>
                    <name and="text" delimiter=", "/>
                  </names>
                </group>
              </if>
            </choose>
          </group>
          <names delimiter=", " variable="editor translator">
            <label form="verb-short" suffix=" " text-case="lowercase"/>
            <name and="text" delimiter=", "/>
          </names>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="edition-note">
    <choose>
      <if match="any" type="bill book graphic legal_case motion_picture report song chapter paper-conference">
        <text macro="edition-action"/>
      </if>
    </choose>
  </macro>
  <macro name="edition-action">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <number suffix="." variable="edition"/>
      </else>
    </choose>    
  </macro>
  <macro name="director-note">
    <group delimiter=" ">
      <text form="verb-short" term="director" text-case="lowercase"/>
      <names variable="author">
        <name and="text" delimiter=", "/>
      </names>
    </group>
  </macro>
  <macro name="description-note">
    <group delimiter=", ">
      <text macro="interviewer-note"/>
      <choose>
        <if match="none" variable="title"/>
        <else-if match="any" type="thesis speech"/>
        <else-if type="graphic broadcast video motion_picture song"/>
        <else>
          <group delimiter=" ">
            <text form="short" variable="medium"/>
            <text form="short" variable="genre"/>
          </group>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="description-multimedia-note">
    <group delimiter=", ">
      <choose>
        <if type="graphic">
          <group delimiter=", ">
            <text form="short" variable="medium"/>
            <text form="short" variable="dimensions"/>
          </group>
        </if>
        <else-if type="broadcast video song motion_picture">
          <group delimiter=", ">
            <text form="short" variable="medium"/>
            <text form="short" variable="genre"/>
          </group>
        </else-if>
      </choose>
    </group>
  </macro>
  <macro name="contributors-note">
    <group delimiter=" ">
      <choose>
        <if match="none" type="broadcast">
          <names variable="author">
            <name and="text" delimiter=", " sort-separator=", "/>
            <label form="short" prefix=", "/>
            <substitute>
              <names variable="composer"/>
              <names variable="container-author"/>
              <names variable="editor"/>
              <text macro="authority-macro"/>
              <names variable="translator"/>
            </substitute>
          </names>
        </if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="container-title-note">
    <group delimiter=" ">
      <choose>
        <if match="any" type="chapter">
          <text term="in" text-case="lowercase"/>
        </if>
        <else-if type="paper-conference" variable="event container-title"/>
        <else-if type="paper-conference">
          <text term="in" text-case="lowercase"/>
        </else-if>
      </choose>
      <choose>
        <if match="none" type="legal_case">
          <text font-style="italic" form="short" variable="container-title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="container-contributors-note">
    <choose>
      <if match="any" type="chapter paper-conference">
        <text macro="editor-translator"/>
      </if>
    </choose>
  </macro>
  <macro name="collection-title">
    <choose>
      <if type="song">
        <choose>
          <if variable="container-title">
            <group delimiter=" ">
              <text variable="collection-title"/>
              <number variable="collection-number"/>
            </group>
          </if>
          <else>
            <group delimiter=" " font-style="italic">
              <text variable="collection-title"/>
              <number variable="collection-number"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <group delimiter=" ">
          <text variable="collection-title"/>
          <number variable="collection-number"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="archive-note">
    <choose>
      <if type="thesis">
        <group delimiter=" ">
          <text form="short" variable="archive"/>
          <text prefix="(" suffix=")" variable="archive_location"/>
        </group>
      </if>
      <else-if match="none" type="report" variable="jurisdiction">
        <group delimiter=", ">
          <text variable="archive_location"/>
          <text form="short" variable="archive"/>
          <text form="short" variable="archive-place"/>
          <choose>
            <if type="manuscript">
              <text variable="publisher-place" form="short"/>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="access-note">
    <group delimiter=", ">
      <choose>
        <if match="any" type="graphic report">
          <text macro="archive-note"/>
        </if>
        <else-if match="none" type="bill book graphic legal_case motion_picture report song article-journal article-magazine article-newspaper chapter paper-conference">
          <text macro="archive-note"/>
        </else-if>
      </choose>
      <choose>
        <if match="none" type="legal_case">
		  <choose>
			<if variable="URL">
			  <group delimiter=" ">
				<text variable="URL"/>
				<group delimiter=" " prefix="(" suffix=")">
				  <text term="accessed"/>
				  <date date-parts="year-month-day" form="text" variable="accessed"/>
				</group>
			  </group>
			</if>
		  </choose>
        </if>
      </choose>
    </group>
  </macro>
  <citation disambiguate-add-names="true" et-al-min="3" et-al-use-first="1">
    <layout delimiter="。" locale="ja zh kr" suffix="。">
      <choose>
        <if position="ibid-with-locator">
          <group delimiter=" ">
            <text value="同"/>
            <group>
              <text variable="locator"/>
              <label variable="locator"/>
            </group>
          </group>
        </if>
        <else-if position="ibid">
          <text value="同上"/>
        </else-if>
        <else-if position="subsequent">
          <text macro="hhkk-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="hhkk-secondary-first"/>
        </else>
      </choose>
    </layout>
    <layout delimiter="; " suffix=".">
      <choose>
        <if>
          <conditions match="any">
            <condition match="any" type="treaty legal_case regulation legislation bill hearing gazette patent"/>
            <condition match="all" type="report" variable="jurisdiction"/>
          </conditions>
          <text macro="legal-types-filter"/>
        </if>
        <else-if position="ibid-with-locator">
          <text macro="ibid-with-locator"/>
        </else-if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if>
          <conditions match="all">
            <condition match="all" position="subsequent"/>
            <condition match="none" type="classic"/>
          </conditions>
          <text macro="cms-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="cms-secondary-first"/>
        </else>
      </choose>
    </layout>
  </citation>
</style>