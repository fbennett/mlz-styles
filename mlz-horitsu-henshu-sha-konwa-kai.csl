  </info>
  <locale>
    <date form="imperial">
      <date-part form="imperial" name="year" suffix="年"/>
      <date-part form="numeric" name="month" suffix="月"/>
      <date-part form="numeric" name="day" suffix="日"/>
    </date>
    <terms>
      <term name="and" form="symbol">&amp;</term>
    </terms>
  </locale>
  <locale xml:lang="en">
    <terms>
      <term form="long" name="rule">rule</term>
      <term form="short" name="rule">r.</term>
      <term name="presented at">presented at</term>
      <term name="article">article</term>
      <term name="director">director</term>
      <term form="verb" name="director">directed by</term>
      <term form="verb-short" name="director">dir.</term>
      <term form="short" name="director">dir.</term>
      <term form="long" name="article">
        <single>article</single>
        <multiple>articles</multiple>
      </term>
      <term form="short" name="article">
        <single>art.</single>
        <multiple>arts.</multiple>
      </term>
      <term form="long" name="Section">
        <single>section</single>
        <multiple>sections</multiple>
      </term>
      <term form="short" name="Section">
        <single>sec.</single>
        <multiple>secs.</multiple>
      </term>
      <term form="long" name="Chapter">
        <single>chapter</single>
        <multiple>chapters</multiple>
      </term>
      <term form="short" name="Chapter">
        <single>ch.</single>
        <multiple>chs.</multiple>
      </term>
      <term form="symbol" name="Chapter">
        <single>c.</single>
        <multiple>cc.</multiple>
      </term>
      <term name="title">title</term>
      <term form="short" name="title">tit.</term>
      <term form="short" name="section">sec.</term>
      <term form="symbol" name="section">
        <single>§</single>
        <multiple>§§</multiple>
      </term>
      <term form="verb-short" name="editortranslator">
        <single>trans. and ed.</single>
        <multiple>trans. and ed.</multiple>
      </term>

      <term form="verb-short" name="director">dir.</term>
      <term form="verb-short" name="editor">ed.</term>
      <term form="verb-short" name="editorial-director">ed.</term>
      <term form="verb-short" name="illustrator">illus.</term>
      <term form="verb-short" name="translator">trans.</term>
      <term form="verb-short" name="editortranslator">ed. &amp; trans.</term>

      <term form="verb" name="editortranslator">
        <single> Translated and edited by</single>
        <multiple> Translated and edited by</multiple>
      </term>
    </terms>
  </locale>
  <locale xml:lang="ja">
    <terms>
      <term name="page">頁</term>
      <term name="et-al">ほか</term>
      <term name="editor">編</term>
      <term name="translator">訳</term>
      <term name="volume">巻</term>
      <term name="issue">号</term>
    </terms>
    <date form="imperial">
      <date-part form="imperial" name="year" suffix="年"/>
      <date-part form="numeric" name="month" suffix="月"/>
      <date-part form="numeric" name="day" suffix="日"/>
    </date>
  </locale>
  <macro name="hhkk-title">
    <choose>
      <if>
        <conditions match="any">
          <condition variable="container-title"/>
          <condition match="all" variable="volume-title collection-title"/>
          <condition match="all" variable="collection-number collection-title"/>
          <condition match="all" variable="volume collection-title"/>
          <condition type="legislation regulation gazette"/>
        </conditions>
        <text prefix="「" suffix="」" variable="title"/>
      </if>
      <else-if match="none" variable="container-title volume-title collection-title">
        <group delimiter=" " prefix="『" suffix="』">
          <text variable="title"/>
          <number prefix="(" suffix=")" variable="volume"/>
        </group>
      </else-if>
      <else>
        <text prefix="『" suffix="』" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="hhkk-volume-chain">
    <group delimiter=" ">
      <text variable="container-title"/>
      <group>
        <number variable="volume"/>
        <text term="volume"/>
      </group>
      <group>
        <number variable="issue"/>
        <label variable="issue"/>
      </group>
      <group>
        <number variable="page"/>
        <label variable="page"/>
      </group>
    </group>
    <group prefix="〔" suffix="〕">
      <text variable="locator"/>
      <label variable="locator"/>
    </group>
  </macro>
  <macro name="hhkk-author-first">
    <choose>
      <if match="all" type="legal_case" variable="commenter">
        <names variable="dummy">
          <name/>
          <substitute>
            <names variable="commenter"/>
          </substitute>
        </names>
        <text macro="hhkk-translator-supp"/>
      </if>
      <else-if>
        <conditions match="all">
          <condition match="any" position="subsequent" variable="title"/>
          <condition variable="author editor" match="all"/>
        </conditions>
        <names variable="dummy">
          <name/>
          <label/>
          <substitute>
            <names variable="editor"/>
          </substitute>
        </names>
        <text macro="hhkk-translator-supp"/>
      </else-if>
      <else-if>
        <conditions match="all">
          <condition match="any" position="subsequent" variable="title"/>
          <condition variable="author editor" match="nand"/>
        </conditions>
        <names variable="dummy">
          <name/>
          <label/>
          <substitute>
            <names variable="author"/>
            <names variable="editor"/>
          </substitute>
        </names>
        <text macro="hhkk-translator-supp"/>
      </else-if>
      <else>
        <names variable="translator">
          <name/>
          <label/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="hhkk-translator-supp">
    <names variable="translator" prefix="（" suffix="）">
      <name/>
      <label/>
    </names>
  </macro>
  <macro name="sist02-patent">
    <group>
      <text variable="title" prefix="「" suffix="」"/>
      <group delimiter="、">
        <group delimiter="、">
          <choose>
            <if jurisdiction="jp">
              <group>
                <text value="特開"/>
                <number variable="publication-number"/>
              </group>
            </if>
            <else-if variable="publication-number">
              <group delimiter=" ">
                <text form="short" variable="jurisdiction"/>
                <text value="Patent Pub."/>
                <number variable="publication-number"/>
              </group>
            </else-if>
          </choose>
        </group>
        <date date-parts="year-month-day" form="text" variable="publication-date"/>
        <group delimiter=", ">
          <choose>
            <if jurisdiction="jp">
              <group>
                <text value="特許第"/>
                <number variable="number"/>
                <text value="号"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <text form="short" variable="jurisdiction"/>
                <text value="Patent"/>
                <number variable="number"/>
              </group>
            </else>
          </choose>
        </group>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </group>
    </group>
  </macro>
  <macro name="hhkk-court-short">
    <choose>
      <if subjurisdictions="1">
        <text form="short" variable="jurisdiction"/>
      </if>
      <else>
        <names variable="authority">
          <name/>
          <institution institution-parts="short"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="hhkk-court-date">
    <choose>
      <if jurisdiction="jp">
        <date date-parts="year-month-day" form="imperial" variable="issued"/>
      </if>
      <else>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </else>
    </choose>
  </macro>
  <macro name="hhkk-secondary-first">
    <text macro="hhkk-author-first"/>
    <choose>
      <if type="legislation regulation gazette">
        <text macro="hhkk-title"/>
      </if>
    </choose>
    <choose>
      <if type="legal_case legislation regulation gazette">
        <choose>
          <if variable="commenter">
            <text macro="hhkk-author-first"/>
            <group delimiter="：" prefix="「" suffix="」">
              <text value="判批"/>
              <group>
                <text macro="hhkk-court-short"/>
                <text macro="hhkk-court-date"/>
              </group>
            </group>
          </if>
          <else>
            <text macro="hhkk-court-short"/>
            <text macro="hhkk-court-date"/>
          </else>
        </choose>
        <text macro="hhkk-volume-chain"/>
      </if>
      <else-if type="patent">
        <text macro="sist02-patent"/>
      </else-if>
      <else-if type="book chapter">
        <text macro="hhkk-title"/>
        <choose>
          <if match="any" variable="container-title volume-title collection-title">
            <names delimiter="、" variable="container-author editor recipient">
              <name/>
              <label/>
            </names>
            <choose>
              <if variable="recipient">
                <text value="先生"/>
                <number variable="edition"/>
              </if>
            </choose>
            <group delimiter=" " prefix="『" suffix="』">
              <text variable="container-title"/>
              <choose>
                <if match="none" variable="volume-title collection-title">
                  <number prefix="(" suffix=")" variable="volume"/>
                </if>
              </choose>
            </group>
          </if>
        </choose>
        <group prefix="『" suffix="』">
          <choose>
            <if match="all" variable="volume-title collection-title collection-number">
              <text variable="collection-title"/>
              <number variable="collection-number"/>
              <text variable="volume-title"/>
            </if>
            <else-if match="any" variable="volume-title collection-title">
              <group delimiter=" ">
                <group delimiter="・">
                  <text variable="collection-title"/>
                  <text variable="volume-title"/>
                </group>
                <number prefix="(" suffix=")" variable="volume"/>
              </group>
            </else-if>
          </choose>
        </group>
        <group delimiter="、" prefix="（" suffix="）">
          <text variable="publisher"/>
          <group>
            <choose>
              <if match="none" variable="recipient">
                <text value="第"/>
                <number variable="edition"/>
                <text value="版"/>
              </if>
            </choose>
          </group>
          <date date-parts="year" form="text" variable="issued"/>
        </group>
        <group>
          <text variable="locator"/>
          <label variable="locator"/>
        </group>
        <choose>
          <if match="none" variable="title">
            <names prefix="〔" suffix="〕" variable="author">
              <name form="short"/>
              <label/>
            </names>
          </if>
          <else-if match="none" variable="container-title volume-title collection-title">
            <names prefix="〔" suffix="〕" variable="author">
              <name/>
              <label/>
            </names>
          </else-if>
        </choose>
      </else-if>
      <else>
          <text macro="hhkk-title"/>
          <group delimiter=" ">
            <group>
              <text macro="hhkk-volume-chain"/>
              <text prefix="〔" suffix="〕" variable="event-place"/>
            </group>
            <date date-parts="year" form="text" prefix="(" suffix=")" variable="issued"/>
          </group>
      </else>
    </choose>
  </macro>
  <macro name="hhkk-secondary-subsequent">
    <choose>
      <if type="legal_case legislation regulation gazette">
        <text macro="hhkk-secondary-first"/>
      </if>
      <else>
        <group delimiter="・">
          <text macro="hhkk-author-first"/>
          <group delimiter=" ">
            <text value="前掲注"/>
            <text prefix="(" suffix=")" variable="first-reference-note-number"/>
          </group>
        </group>
        <text macro="hhkk-title"/>
        <group>
          <number variable="locator"/>
          <label variable="locator"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="legal-types-filter">
    <choose>
      <if position="ibid-with-locator">
        <choose>
          <if type="legal_case hearing patent">
            <group delimiter=", ">
              <text term="ibid"/>
              <text label-form="short" macro="std-locator-non-page-label"/>
            </group>
          </if>
          <else-if type="bill">
            <group delimiter=", ">
              <text term="ibid"/>
              <text label-form="symbol" macro="std-locator-label"/>
            </group>
          </else-if>
          <else>
            
            <group delimiter=", ">
              <text term="ibid"/>
              <text label-form="symbol" macro="std-locator-non-page-label"/>
            </group>
          </else>
        </choose>
      </if>
      <else-if position="ibid">
        <text term="ibid"/>
      </else-if>
      <else-if type="legal_case">
        <text macro="jurisdictions-case"/>
      </else-if>
      <else-if type="bill">
        <text macro="jurisdictions-bill"/>
      </else-if>
      <else-if match="any" type="gazette legislation regulation">
        <text macro="jurisdictions-legislation"/>
      </else-if>
      <else-if type="hearing">
        <choose>
          <if position="subsequent">
            <text macro="cms-secondary-subsequent"/>
          </if>
          <else>
            <text macro="jurisdictions-hearing"/>
          </else>
        </choose>
      </else-if>
      <else-if type="report">
        <text macro="jurisdictions-report"/>
      </else-if>
      <else-if type="treaty">
        <text macro="jurisdictions-treaty"/>
      </else-if>
      <else-if type="patent">
        <text macro="jurisdictions-patent"/>
      </else-if>
      <else-if position="subsequent">
        <text macro="cms-secondary-subsequent"/>
      </else-if>
      <else>
        <text macro="cms-secondary-first"/>
      </else>
    </choose>
  </macro>

  <macro name="wisconsin-treaty-subsequent">
    <group delimiter=", ">
      <text form="short" variable="title"/>
      <choose>
        <if disambiguate="true">
          <names variable="author">
            <name/>
            <institution and="none" delimiter="–" institution-parts="short"/>
          </names>
        </if>
      </choose>
      <text label-form="symbol" macro="std-locator-non-page-label"/>
      <group delimiter=" ">
        <text font-style="italic" value="supra"/>
        <text value="note"/>
        <text variable="first-reference-note-number"/>
      </group>
      <group delimiter=" " is-parallel="true">
        <number variable="volume"/>
        <text variable="container-title"/>
        <number variable="page-first"/>
      </group>
      <group delimiter=" " is-parallel="false">
        <text value="at"/>
        <number variable="page-first"/>
      </group>
    </group>
  </macro>
  <macro name="wisconsin-treaty-first">
    <group delimiter=" ">
      <group delimiter=", ">
        <text form="short" variable="title"/>
        <names variable="author">
          <name/>
          <institution delimiter="-" institution-parts="short"/>
        </names>
        <text label-form="symbol" macro="std-locator-non-page-label"/>
        <choose>
          <if variable="event-date">
            
            <date date-parts="year-month-day" form="text" variable="event-date"/>
          </if>
          <else>
            
            <group delimiter=" ">
              <text font-style="italic" value="opened for signature"/>
              <date date-parts="year-month-day" form="text" variable="available-date"/>
            </group>
          </else>
        </choose>
        <group delimiter=" ">
          <number variable="volume"/>
          <text form="short" variable="container-title"/>
          <number variable="page-first"/>
        </group>
      </group>
      <group delimiter=" " prefix="(" suffix=")">
        <text value="entered into force"/>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </group>
    </group>
  </macro>
  <macro name="wisconsin-supra-stub">
    <group delimiter=", ">
      <group delimiter=" ">
        <group delimiter=" ">
          <text font-style="italic" value="supra"/>
          <choose>
            <if variable="first-reference-note-number">
              <text value="note"/>
              <text variable="first-reference-note-number"/>
            </if>
          </choose>
        </group>
        <choose>
          <if match="all" variable="first-reference-note-number locator"/>
          <else>
            <text label-form="short" macro="std-locator-paragraph-bracket-at"/>
          </else>
        </choose>
      </group>
      <text label-form="short" macro="std-locator-paragraph-bracket-at"/>
    </group>
  </macro>
  
  <macro name="us-title-short">
    <choose>
      <if>
        <conditions match="any">
          <condition match="none" variable="author composer editor"/>
          <condition disambiguate="true" match="any" type="motion_picture"/>
        </conditions>
        <choose>
          <if match="all" type="bill gazette legislation" variable="hereinafter">
            <text form="short" variable="hereinafter"/>
          </if>
          <else-if match="all" type="legal_case" variable="hereinafter">
            <text font-style="italic" form="short" variable="hereinafter"/>
          </else-if>
          <else-if match="all" type="legal_case" variable="title-short">
            <text font-style="italic" form="short" variable="title-short"/>
          </else-if>
          <else-if match="all" type="legal_case">
            <text font-style="italic" form="short" variable="title"/>
          </else-if>
          <else-if match="all" type="bill gazette legislation">
            <text form="short" variable="title"/>
          </else-if>
          <else>
            <text macro="us-default-title"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>

  <macro name="us-slip-opinion-locator">
    <group delimiter=", ">
      <group delimiter=" ">
        <text value="No."/>
        <number text-case="normal" variable="number"/>
      </group>
      <group delimiter=" ">
        <text form="short" variable="archive"/>
        <text variable="archive_location"/>
      </group>
      <group delimiter=" ">
        <choose>
          <if variable="archive"/>
          <else>
            <text value="slip op."/>
          </else>
        </choose>
        <text label-form="symbol" macro="std-locator-non-page-label-at"/>
      </group>
    </group>
  </macro>
  <macro name="us-session-number">
    <group delimiter=" ">
      <number form="ordinal" variable="chapter-number"/>
      <choose>
        <if variable="genre">
          <text form="short" variable="genre"/>
        </if>
        <else>
          <text value="Sess."/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="us-report-subsequent">
    <text value="[us-report-subsequent]"/>
  </macro>

  <macro name="us-report-first">
    <text value="[us-report-first]"/>
  </macro>

  <macro name="us-pinpoint-action">
    <group delimiter=" ">
      <choose>
        <if locator="page" match="none" type="personal_communication">
          <text label-form="symbol" macro="std-locator-non-page-label"/>
        </if>
        <else-if>
          <conditions match="any">
            <condition position="ibid-with-locator"/>
            <condition is-numeric="title" match="all" type="book" variable="title"/>
            <condition match="all" type="song" variable="section"/>
            <condition is-numeric="title" match="all" type="song" variable="title locator"/>
          </conditions>
          <text label-form="symbol" macro="std-locator-non-page-label-at"/>
        </else-if>
        <else-if type="personal_communication">
          <text macro="std-locator-non-page-label-at"/>
        </else-if>
        <else-if>
          <conditions match="all">
            <condition type="legal_case" position="subsequent" variable="locator" match="all"/>
            <condition position="near-note" match="all"/>
          </conditions>
          <text macro="std-locator-non-page-label-at"/>
        </else-if>
        <else>
          <number variable="locator"/>
        </else>
      </choose>
    </group>
  </macro>
  
  <macro name="us-legislation-subsequent">
    <choose>
      <if match="any" variable="title container-title">
        <group delimiter=", ">
          <text variable="title"/>
          <group delimiter=" ">
            <group delimiter=" " is-parallel="true">
              <text macro="us-code-detail"/>
            </group>
            <text label-form="symbol" macro="std-locator-non-page-label"/>
          </group>
        </group>
      </if>
      <else>
        <text macro="us-legislation-first"/>
      </else>
    </choose>
  </macro>

  <macro name="us-legislation-first">
    <group delimiter=" ">
      <group delimiter=", ">
        <text variable="title"/>
        <choose>
          <if type="gazette">
            <group delimiter=" ">
              <choose>
                <if is-numeric="number">
                  <text value="Pub. L. No."/>
                </if>
              </choose>
              <number label-form="short" variable="number"/>
            </group>
          </if>
        </choose>
        <group delimiter=" ">
          <text macro="us-code-detail"/>
          <number variable="page"/>
          <choose>
            <if locator="page" match="none">
              <text label-form="symbol" macro="std-locator-non-page-label"/>
            </if>
          </choose>
        </group>
        <text label-form="symbol" macro="std-locator-non-page-label"/>
        <choose>
          <if variable="number">
            <text label-form="symbol" macro="std-locator-non-page-label"/>
          </if>
          <else-if locator="page" match="all" variable="page">
            <text label-form="symbol" macro="std-locator-non-page-label"/>
          </else-if>
          <else-if variable="container-title"/>
          <else-if locator="section paragraph"/>
          <else-if match="none" variable="container-title">
            <text label-form="symbol" macro="std-locator-non-page-label"/>
          </else-if>
        </choose>
      </group>
      <text label-form="symbol" macro="std-locator-non-page-label"/>
      <group delimiter=" " prefix="(" suffix=")">
        <text form="short" variable="publisher"/>
        <choose>
          <if variable="publication-date">
            <date date-parts="year" form="text" variable="publication-date"/>
          </if>
          <else>
            <date date-parts="year" form="text" variable="issued"/>
          </else>
        </choose>
      </group>
    </group>
  </macro>
  <macro name="us-hearing-front">
    <group delimiter="" quotes="true">
      <choose>
        <if position="near-note">
          <text form="short" variable="title"/>
        </if>
        <else>
          <text variable="title"/>
        </else>
      </choose>
      <group delimiter=" ">
        <text value=": Hearing"/>
        <group delimiter=" ">
          <text value="on"/>
          <number variable="number"/>
        </group>
        <group delimiter=" ">
          <text value="Before the"/>
          <text macro="std-authority-child"/>
        </group>
      </group>
    </group>
  </macro>
  <macro name="us-default-title">
    <choose>
      <if>
        <conditions match="any">
          <condition match="all" type="song" variable="collection-title"/>
          <condition match="all" type="paper-conference post post-weblog webpage" variable="container-title"/>
          <condition match="any" type="article-magazine graphic article-journal article-newspaper"/>
          <condition match="all" type="chapter entry-encyclopedia entry-dictionary" variable="author"/>
        </conditions>
        
        <text font-style="italic" form="short" text-case="title" variable="title"/>
      </if>
      <else-if match="any" type="paper-conference">
        
        <text form="short" text-case="title" variable="title"/>
      </else-if>
      <else-if type="personal_communication">
        
        <text form="short" variable="title"/>
      </else-if>
      <else-if match="any" type="broadcast video" variable="container-title">
        <text font-style="italic" form="short" variable="title"/>
      </else-if>
      <else-if match="all" type="broadcast video" variable="container-title publisher">
        
        <group delimiter=": " font-style="italic">
          <text form="short" text-case="title" variable="container-title"/>
          <text form="short" text-case="title" variable="title"/>
        </group>
      </else-if>
      <else-if match="any" type="post post-weblog webpage broadcast">
        
        <text font-variant="small-caps" form="short" text-case="title" variable="title"/>
      </else-if>
      <else-if match="all" type="song" variable="container-title">
        
        <group delimiter=" ">
          <text font-style="italic" form="short" text-case="title" variable="title"/>
          <names prefix="[" suffix="]" variable="composer">
            <name form="short" initialize-with="."/>
          </names>
        </group>
      </else-if>
      <else-if>
        <conditions match="any">
          <condition disambiguate="true" match="all" type="book song motion_picture" variable="title version"/>
          <condition match="all" position="first" type="book song motion_picture" variable="title version"/>
        </conditions>
        <group delimiter=", ">
          <choose>
            <if is-numeric="version">
              <group delimiter=" ">
                <text value="ver."/>
                <text variable="version"/>
              </group>
              <text form="short" variable="medium"/>
            </if>
            <else>
              <text quotes="true" variable="version"/>
              <text form="short" variable="medium"/>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="book song motion_picture">
        
        <text font-variant="small-caps" form="short" text-case="title" variable="title"/>
      </else-if>
      <else>
        
        <text form="short" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>


  <macro name="us-code-detail">
    <group delimiter=" ">
